<div class="randomBeer">


{{#each beersFromApi}}
<div>
  <div>
    <img src={{ image_url }}>
  </div>
  <div class="textBeer">
    <h6>{{ name}}</h6>
    <p> {{ description }} </p>
    <p> {{ tagline }} </p>
    <p> {{ food_pairing }} </p>
    <p> {{ brewer_tips }} </p>
  </div>

</div>
{{/each}}

</div>